# ============================================
# SUPABASE CONFIGURATION
# ============================================

# Para desarrollo local (supabase start):
# NEXT_PUBLIC_SUPABASE_URL=http://127.0.0.1:54321
# NEXT_PUBLIC_SUPABASE_ANON_KEY=<key from supabase start>
# SUPABASE_SERVICE_ROLE_KEY=<key from supabase start>

# Para producción/staging (Supabase Cloud):
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# ============================================
# STRIPE CONFIGURATION
# ============================================
STRIPE_SECRET_KEY=sk_test_your_secret_key
STRIPE_PUBLIC_KEY=pk_test_your_public_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# ============================================
# APP URLS
# ============================================
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_CLIENT_URL=http://localhost:3000

# ============================================
# EMAIL SERVICE (Resend)
# ============================================
RESEND_API_KEY=re_your_api_key

# ============================================
# OPTIONAL: OpenAI for Supabase Studio AI
# ============================================
# OPENAI_API_KEY=sk-your-openai-key
